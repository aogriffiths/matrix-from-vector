{
  "name": "matrix-from-vector",
  "version": "1.0.0",
  "description": "Create 2D matricies from 1D vectors",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "build:all": "npm run test && npm run build:tsc && npm run build:api-extract && npm run build:api-docs && npm run build:layout-docs",
    "build:tsc": "tsc",
    "build:tsc-watch": "tsc -w",
    "build:api-extract": "mkdir -p etc; api-extractor run --local --verbose",
    "build:api-docs": "api-documenter markdown --input-folder temp --output-folder docs/generated/api",
    "build:layout-docs": "node dist/generatelayouts.js",
    "prepublish": "npm run build:all",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "author": "Adam Griffiths",
  "license": "ISC",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.12.9",
    "@microsoft/api-extractor": "^7.13.2",
    "@types/chai": "^4.2.15",
    "@types/d3": "^6.3.0",
    "@types/jsdom": "^16.2.6",
    "@types/mocha": "^8.2.1",
    "chai": "^4.3.0",
    "handlebars": "^4.7.7",
    "mocha": "^8.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "d3": "^6.5.0",
    "jsdom": "^16.4.0",
    "request": "^2.88.2"
  }
}
